{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@lib/utils(|/.*)$": "<rootDir>/../../../libs/utils/src/$1",
    "@lib/rabbitmq-queue(|/.*)$": "<rootDir>/../../../libs/rabbitmq-queue/src/$1"
  }
}
